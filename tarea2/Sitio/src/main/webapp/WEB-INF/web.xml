<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>TestAplicacionWebDinamica</display-name>
  <welcome-file-list>
    <welcome-file>home</welcome-file>
  </welcome-file-list>
  <error-page>
  	<exception-type>java.lang.Throwable</exception-type>
  	<location>/htmls/error.jsp</location>
  </error-page>
  <error-page>
  	<error-code>404</error-code>
  	<location>/htmls/error404.jsp</location>
  </error-page>
  <context-param>
    <param-name>filedir</param-name>
    <param-value>img</param-value>
  </context-param>
  <context-param>
    <param-name>defaultImgAct</param-name>
    <param-value>default_actividades.jpg</param-value>
  </context-param>
  <context-param>
    <param-name>defaultImgUser</param-name>
    <param-value>default_usuario.png</param-value>
  </context-param>
</web-app>